{
  "dataset": "cxr14",
  "init_cls": 5,
  "increment": 3,
  "backbone_type": "vit_base_patch16_224_lkcil",
  "device": ["0"],
  "model_name": "lkcil",

  "optimizer": "sgd",
  "scheduler": "cosine",

  "prompt_json_path": "./data/CXR14/prompts.json",
  "text_feats_path": "./data/CXR14/text_feats.npy",
  "text_feats_keys_path": "./data/CXR14/text_feats_keys.json",

  "alpha": 0.5,
  "beta": 0.3,
  "lambda_orth": 0.3,
  "lambda_align": 0.3,

  "graph_hidden_dim": 128,
  "graph_sparsity_k": 6,
  "graph_tau": 0.5,
  "graph_alpha": 0.7,

  "tuned_epoch": 20,

  "init_lr": 0.03,
  "batch_size": 32,
  "weight_decay": 0.0005,
  "min_lr": 0,

  "adapter_momentum": 0.1,
  "ensemble": true,

  "pretrained": true,
  "drop": 0.0,
  "drop_path": 0.0,
  "ffn_num": 16,

  "crct_epochs": 20,
  "ca_lr": 0.005,
  "ca_storage_efficient_method": "covariance",
  "ca_storage_efficient_method_choices": ["covariance", "multi-centroid", "variance"],
  "n_centroids": 10,

  "prefix": "",
  "memory_size": 0,
  "memory_per_class": 0,
  "fixed_memory": false,
  "shuffle": false
}
